@model BanHaiSan.ViewModel.EditCate
@{
    ViewBag.Title = "index";
}

<div class="header-wrapper">
    <div class="header-title">
        <h2>Thêm Loại Hải Sản</h2>
    </div>
    <div class="use-info">
    </div>
</div>
<div class="card-container">

    <div style="text-align: left">
        <form enctype="multipart/form-data">
        </form>
        @using (Html.BeginForm("SaveCate", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form-group">
                <label>@Html.LabelFor(x => x.MaLoai)</label>
                @Html.TextBoxFor(x => x.MaLoai, new { @class = "form-control", @readonly = "readonly" })
                @Html.ValidationMessageFor(x => x.MaLoai, "", new { @class = "text-danger" })
            </div>
            <div class="form-group">
                <label>@Html.LabelFor(x => x.TenLoai)</label>
                @Html.TextBoxFor(x => x.TenLoai, new { @class = "form-control", @required = "required" })
                @Html.ValidationMessageFor(x => x.TenLoai, "", new { @class = "text-danger" })
            </div>
            <div class="form-group">
                <input type="submit" value="Lưu" class="btn btn-success" />
            </div>

        }
    </div>
</div>